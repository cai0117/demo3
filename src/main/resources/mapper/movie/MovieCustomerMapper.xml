<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.movie.mapper.MovieCustomerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.movie.model.MovieCustomer" >
        <id column="customer_id" property="customerId" />
        <result column="name" property="name" />
        <result column="tel" property="tel" />
        <result column="origin" property="origin" />
        <result column="nickname" property="nickname" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
        <result column="gender" property="gender" />
        <result column="province_region_id" property="provinceRegionId" />
        <result column="city_region_id" property="cityRegionId" />
        <result column="avatar_url" property="avatarUrl" />
        <result column="movie"  property="movie" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
    </resultMap>

</mapper>
